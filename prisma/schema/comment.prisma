model Comment{
    id Int @id @default(autoincrement())
    content String
    createdBy Int
    user User @relation(fields: [createdBy],references: [id])
    productId Int 
    product Product @relation(fields: [productId], references: [id])
    createdAt DateTime @default(now())

    @@unique([createdBy,productId])
    @@map("comment")
}